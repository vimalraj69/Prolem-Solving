class Solution{
    public void pattern(int n){
        int i=1;
        int m=1;
        while(i<=n){
           for(int j=1;j<=i;j++){
               System.out.print(m);
               m++;
           }for (int k=m-2;k>0;k--){
                System.out.print(k);
            }m=1;i++;
            System.out.println();
        }
    }
    public static void main(String args []){
        Solution s= new Solution();
        s.pattern(6);
    }
}
