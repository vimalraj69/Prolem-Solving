class Solution {
    public int specialArray(int[] nums) {
        
        int i =0;
        
        while(i<=nums.length){
            int count=0;
            for(int j =0;j<nums.length;j++){
                if(i<=nums[j]){
                    count++;
                }
            }
            if(count==i){
                return count;
            }
            i++;
        }
        
        
        return -1;
    }
}
