class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        if(str(word1).equals(str(word2))){
            return true;
        }
        return false;
    }
    public String str(String arr[]){
        String s="";
        for(int i=0;i<arr.length;i++){
            s+=arr[i];
        }
        
        return s;
    }
}
