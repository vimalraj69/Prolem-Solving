class Solution {
    public String finalString(String s) {
        String ans="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='i'){
                ans=reverse( ans);
            }else{
                ans=ans+s.charAt(i);
            }
            
        }
        return ans;
    }

        public String reverse(String s){
            String r="";
            for(int i=0;i<s.length();i++){
                r=s.charAt(i)+r;
            }
            return r;
        }
    
}
