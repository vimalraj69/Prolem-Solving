class Solution {
    public long[] sumOfThree(long num) {
        long n=num/3;
        if((n-1)+n+(n+1)==num){
            long arr[]=new long[3];
            arr[0]=n-1;
            arr[1]=n;
            arr[2]=n+1;
            return arr;
        }
        
        
        return new long[0];
    }
}
